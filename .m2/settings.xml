<settings xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd"
    xmlns="http://maven.apache.org/SETTINGS/1.1.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <mirrors>
        <mirror>
            <id>sproutfx-public</id>
            <mirrorOf>central, public</mirrorOf>
            <name>sproutfx-public</name>
            <url>http://nexus.sproutfx.kr/repository/maven-public/</url>
        </mirror>
        <mirror>
            <id>sproutfx-snapshot</id>
            <mirrorOf>snapshot</mirrorOf>
            <name>sproutfx-snapshot</name>
            <url>http://nexus.sproutfx.kr/repository/maven-snapshots/</url>
        </mirror>
        <mirror>
            <id>sproutfx-release</id>
            <mirrorOf>release</mirrorOf>
            <name>sproutfx-release</name>
            <url>http://nexus.sproutfx.kr/repository/maven-releases/</url>
        </mirror>
    </mirrors>
</settings> 
